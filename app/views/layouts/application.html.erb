<!DOCTYPE html>
<html>
<head>
  <title>Stuff</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="user_nav">
  <div>
  <% if current_user %>
    Logged in as <%= current_user.full_name %>.
    <%= link_to "Log out", logout_path %>
  <% else %>
    <%= link_to "Sign up", signup_path %> |
    <%= link_to "Log in", login_path %>
  <% end %>
  </div>
</div>

<%= yield %>

</body>
</html>
